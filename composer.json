{
    "name": "ellaisys/cognito",
    "description": "AWS Cognito package that allows Auth and other related features using the AWS SDK for PHP",
    "keywords": [
        "aws",
        "cognito",
        "oauth",
        "authentication",
        "user pool",
        "ellaisys"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/ellaisys/aws-cognito",
    "authors": [
        {
            "name": "EllaiSys",
            "email": "support@ellaisys.com",
            "role": "owner",
            "homepage": "https://ellaisys.github.io/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2.5",
        "guzzlehttp/guzzle": "^6.3",
        "aws/aws-sdk-php": "^3.149"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
          "Ellaisys\\Cognito\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ellaisys\\Cognito\\AwsCognitoServiceProvider"
            ],
            "aliases": {
                "ExotelCall": "Ellaisys\\Cognito\\Facades\\AwsCognitoAuthFacade"
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
